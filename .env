# === n8n Configuration ===
N8N_VERSION=latest
N8N_WEBHOOK_URL=https://n8n.example.com

# === PostgreSQL Config for n8n ===
DB_POSTGRESDB_DATABASE=n8n_db
DB_POSTGRESDB_USER=n8n_db
DB_POSTGRESDB_PASSWORD=n8n_pwd

# === Qdrant Configuration ===
QDRANT_API_KEY=supersecret123
ENABLE_QDRANT=false

# === n8n Execution Settings ===
EXECUTIONS_DATA_SAVE_ON_SUCCESS=all
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_MANUAL_EXECUTIONS=true
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=720
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
N8N_RUNNERS_ENABLED=true
OFFLOAD_MANUAL_EXECUTIONS_TO_WORKERS=true

# === Worker Scaling ===
N8N_WORKER_REPLICAS=4

# === NGINX Proxy Manager Configuration ===
ENABLE_NGINX_PROXY_MANAGER=true
NPM_POSTGRES_USER=npm
NPM_POSTGRES_PASSWORD=npmpass
NPM_POSTGRES_DB=npm
